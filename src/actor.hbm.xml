<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
       
<hibernate-mapping>
   <class name="com.run.Actor" table="sakila.actor">
      <id name="actorid" type="int" column="actor_id">
          <generator class="assigned"/>
      </id>   
     <property name="fname"  column="first_name" type="string"></property>
     <property name="lname"  column="last_name" type="string"></property>
     <property name="lstupdate"  column="last_update" type="string"></property>
   </class>
</hibernate-mapping>